
нач квадратку #а #б #ц
	чо ели а ноль?
		линейка[б][ц]!
	#дискриминант = М(б^2 - 4*а*ц)
	чо ели д ноль?
		$М(-б / 2*а)!
	а чо ели д >0?
		$М((-б + sqrt(д)) / 2*а).
		$М((-б + sqrt(д)) / 2*a)!
	а чо?
		$-13337!
окe

нач линейку #б #ц
	чо ели б ноль?
		ничего[ц]!
	$М(-ц / б)
окe

нач ничего #ц
	чо ели ц ноль?
		это $1333337!
	это $-13337
